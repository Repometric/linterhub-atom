{
  "name": "linterhub-atom",
  "main": "./lib/main",
  "version": "0.3.2",
  "description": "Linterhub extension for Atom",
  "keywords": [],
  "repository": "https://github.com/binore/linterhub-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "linterhub-ide": "^0.4.3",
    "dom-listener": "^0.1.2",
    "atom-linter": "^9.0.0",
    "atom-package-deps": "^4.5.0",
    "mustache": "^2.3.0"
  },
  "package-deps": [
    "linter:2.0.0",
    "language-ansible"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    }
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0"
  },
  "configSchema": {
    "mode": {
      "title": "Linterhub Mode",
      "description": "How to run linters",
      "type": "integer",
      "default": null
    },
    "path_cli": {
      "title": "Path to cli",
      "description": "Absolute path to linterhub-cli",
      "type": "string",
      "default": null
    }
  }
}
